.title-search {
  display: flex;
  height: 80rpx;
  margin-top: 8rpx;
  margin-left: 32rpx;
  margin-right: 32rpx;
  border: 1px solid #dddddd;
  border-radius: 5%;
  box-shadow: 6rpx 6rpx 3rpx 6rpx #dddddd;
}

.img-search-icon{
  width: 32rpx;
  height: 32rpx;
  margin-left: 20rpx;
  margin-top: 24rpx;
}

.inp-search {
  height: 60rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  background-color: #fdfdfd
}

.v-divider{
  height: 2rpx;
  background-color: #dddddd;
  margin-top: 20rpx;
  box-shadow: 6rpx 6rpx 3rpx 6rpx #dddddd;
}

.guess-you{
  display: flex;
  margin-top: 20rpx;
}

.v-guess-item {
  display: flex;
}

.btn-guess {
  height: 100rpx;
  margin-left: 16rpx;
  margin-right: 16rpx;
  display: inline-block;
}

.v-help-find{
  margin: 20rpx;
}

.txt-help-find-title{
  font-size: 60rpx;
  font-weight: bold;
}

.scv-help-find{
  margin-top: 20rpx;
  display: flex;
}

.v-help-find-item{
  width: 300rpx;
  height: 300rpx;
  display: inline-block;
  border: 1px solid #dddddd;
  margin-right: 32rpx;
}

.v-help-find-root{
  display: flex;
  flex-flow: column;
}

.img-help-find-item{
  width: 300rpx;
  height: 150rpx;
}

.txt-help-find-item{
  font-size: 40rpx;
  font-weight: bolder;
  margin-left: 10rpx;
}